{
  "-notes": "Peak-detection parameters.",
  "sampling_rate": 44100,
  "channels": [
    {
      "name": "good_motor",
      "zscore_threshold": [0.1, 8]
    },
    {
      "name": "bad_motor",
      "zscore_threshold": [0.1, 8]
    }
  ],
  "stft": {
    "window": "hann",
    "frame_size": 512,
    "hop_size": 128
  },
  "bandpass_hz": [1500, 3000],
  "energy_metric": "power_sum",
  "flux": {
    "method": "positive_diff",
    "smooth_window": 20
  },
  "std_min_threshold": 1e-11
}

