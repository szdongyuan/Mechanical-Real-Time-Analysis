{
  "-notes": "Peak-detection parameters. energy_threshold用于判断电机是否运行，zscore_threshold用于判断是否被敲击",
  "sampling_rate": 44100,
  "channels": [
    {
      "name": "good_motor",
      "zscore_threshold": 8.0,
      "energy_threshold": 1e-6
    },
    {
      "name": "bad_motor",
      "zscore_threshold": 8.0,
      "energy_threshold": 1e-6
    }
  ],
  "defaults": {
    "zscore_threshold": 4.0,
    "energy_threshold": 1e-6
  },
  "stft": {
    "window": "hann",
    "frame_size": 512,
    "hop_size": 128
  },
  "bandpass_hz": [1500, 3000],
  "energy_metric": "power_sum",
  "flux": {
    "method": "positive_diff",
    "smooth_window": 20
  },
  "std_min_threshold": 1e-11
}
