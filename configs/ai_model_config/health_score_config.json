[
  {
    "-notes": "Health-score settings. sleep_range用于停机状态，normal_range用于正常运行，abnormal_range用于被敲击状态",
    "module_name": "health_score",
    "module_config": {
      "precision": 1,
      "random_seed": null,
      "defaults": {
        "sleep_range": [98, 100],
        "normal_range": [90, 95],
        "abnormal_range": [55, 60]
      },
      "channels": {
        "good_motor": {
          "sleep_range": [99, 100],
          "normal_range": [85, 100],
          "abnormal_range": [10, 20]
        },
        "bad_motor": {
          "sleep_range": [99, 100],
          "normal_range": [50, 70],
          "abnormal_range": [0, 10]
        }
      },
      "aggregate": {
        "name": "overall",
        "channels": ["good_motor", "bad_motor"],
        "method": "weighted",
        "weights": [0.5, 0.5]
      }
    }
  }
]
